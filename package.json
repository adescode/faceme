{
  "name": "Faceme",
  "version": "0.0.1",
  "description": "WebRTC video call",
  "engines": {
    "node": "10.x"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "node test.js",
    "livelog": "heroku logs --tail",
    "minifyJS": "uglifyjs main.js --compress --mangle --warn --output main.min.js",
