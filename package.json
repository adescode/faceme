  },
  "dependencies": {
    "express": "^4.17.1",
    "node-static": "^0.7.10",
    "path": "^0.12.7",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "uglify-js": "^3.9.1"
  }
}
